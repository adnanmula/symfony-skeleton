{
    "name": "adnanmula/symfony-skeleton",
    "description": "Symfony skeleton",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "adnanmula/telegram-send-message": "^0.0.5",
        "beberlei/assert": "^3.2",
        "doctrine/dbal": "^2.10",
        "eleirbag89/telegrambotphp": "^1.3",
        "guzzlehttp/guzzle": "^6.5",
        "ramsey/uuid": "^4.0",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "symfony/messenger": "5.1.*",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "adnanmula/coding-standard": "^0.0.4",
        "phpro/grumphp": "^1.2",
        "phpstan/phpstan": "^0.12.30",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "AdnanMula\\Skeleton\\": "src/",
            "AdnanMula\\Skeleton\\Migrations\\": "migrations/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdnanMula\\Skeleton\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "rm -rf .git/hooks",
            "cp -r scripts/git-hooks .git/hooks"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
